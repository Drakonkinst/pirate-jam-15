[sub_resource type="Resource" id="Resource_anqha"]
script = ExtResource("1_v42ue")
speaker = ExtResource("2_mfuvd")
text = "You seem to be getting back into your old habits..."

[sub_resource type="Resource" id="Resource_qsp5h"]
script = ExtResource("1_v42ue")
speaker = ExtResource("3_tpan4")
text = "Old habits?"

[sub_resource type="Resource" id="Resource_38oxr"]
script = ExtResource("1_v42ue")
speaker = ExtResource("2_mfuvd")
text = "You know... throwing things. Brewing your alchemical mixtures. Playing with shadowsand."

[sub_resource type="Resource" id="Resource_5rmsy"]
script = ExtResource("1_v42ue")
speaker = ExtResource("2_mfuvd")
text = "Pretending as if you have any say in how nature carries itself."

[sub_resource type="Resource" id="Resource_gkdba"]
script = ExtResource("1_v42ue")
speaker = ExtResource("2_mfuvd")
text = "Ha. It's like we're kids again!"

[sub_resource type="Resource" id="Resource_clqsd"]
script = ExtResource("1_v42ue")
speaker = ExtResource("3_tpan4")
text = "Don't bring up the past if you're only going to taunt me."

[sub_resource type="Resource" id="Resource_0dbvd"]
script = ExtResource("1_v42ue")
speaker = ExtResource("3_tpan4")
text = "I have no need for any of your mockery."

[sub_resource type="Resource" id="Resource_g73nn"]
script = ExtResource("1_v42ue")
speaker = ExtResource("2_mfuvd")
text = "You're starting to sound just like me when you're angry, you know. "

[sub_resource type="Resource" id="Resource_b1u5o"]
script = ExtResource("1_v42ue")
speaker = ExtResource("2_mfuvd")
text = "If only you'd start acting like me, too. Wouldn't that be fun?"

[sub_resource type="Resource" id="Resource_mvvhj"]
script = ExtResource("1_v42ue")
speaker = ExtResource("3_tpan4")
text = "Enough!"

[sub_resource type="Resource" id="Resource_3t5gn"]
script = ExtResource("1_v42ue")
speaker = ExtResource("3_tpan4")
text = "You're wasting my time. I need to keep moving."

[sub_resource type="Resource" id="Resource_g4w4t"]
script = ExtResource("1_v42ue")
speaker = ExtResource("2_mfuvd")
text = "Then what are you waiting for? Defeat me. "

